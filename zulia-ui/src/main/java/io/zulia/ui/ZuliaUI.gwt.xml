<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='zuliaui'>

    <inherits name='com.google.gwt.user.User'/>
    <inherits name='com.google.gwt.place.Place'/>
    <inherits name="com.google.gwt.i18n.I18N"/>

    <inherits name="gwt.material.design.GwtMaterialWithJQuery"/>
    <inherits name="gwt.material.design.addins.GwtMaterialAddins"/>
    <inherits name="gwt.material.design.themes.ThemeGrey"/>
    <inherits name="gwt.material.design.themes.ThemeGreyResources"/>

    <inherits name="org.mongodb.morphia.Morphia"/>

    <entry-point class='io.zulia.ui.client.ZuliaUI'/>

    <set-configuration-property name="CssResource.enableGss" value="true"/>


    <source path='client'/>
    <source path='shared'/>

</module>
